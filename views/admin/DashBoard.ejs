<div class="container-fluid py-5">
    <h1 class="h3 mb-2 text-gray-800">BẢNG VÙNG MIỀN</h1>
    <div class="row">
        <div class="col-12">
            <div class="card shadow mb-4">
                <!-- Card Header - Dropdown -->
                <div class="card-header py-3">
                    <h6 class="m-0 font-weight-bold text-primary">Thống kê số thành phố theo vùng miền</h6>
                </div>
                <!-- Card Body -->
                <div class="card-body">
                    <div class="chart-custom pt-4">
                        <canvas id="MyCharCityOfRegion" data-dataNorth=<%=dataNorth %> data-dataSouth=<%=dataSouth %> data-dataWestern=<%=dataWestern %>></canvas>
                    </div>
                    <hr>
                    
                </div>
            </div>
        </div>
    </div>
</div>
