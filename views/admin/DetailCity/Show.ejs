<div class='AdminProduct admin-detail-city'>
    <div class="container">
        <h2 class="admin-title">
            Chi tiết thành phố
        </h2>
        <div class="admin-control">
            <a href="/admin/DetailCity/create" class="admin-controll-add btn btn-primary">
                Thêm chi tiết thành phố
            </a>
        </div>
        <div class="AdminProduct-list">
            <div class="row">
                <div class="col-12 mb-5">
                    <select class="form-select" aria-label="Default select example">
                        <option selected>Chọn thành phố</option>
                        <% citys.forEach(function(city){ %>
                            <option value="<%=city.dataValues.IDC%>"><%=city.dataValues.Name%></option>
                                <% }) %>
                      </select>
                </div>
                <div class="col-12">
                    <div class="table-responsive wishlist_table">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th class="product-name">Năm</th>
                                    <th class="product-price">Loại thành phố</th>
                                    <th class="product-add-to-cart">Dân số</th>
                                    <th class="product-remove">Diện tích</th>
                                    <th class="product-remove">Tốc độ phát triển công nghiệp</th>
                                    <th class="product-remove">Tọa độ</th>
                                    <th class="product-add-to-cart">Sửa</th>
                                    <th class="product-remove">Xóa</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js" integrity="sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>

<script>
        $(function(){
            $("select").change((e)=>{
                $.post("/admin/DetailCity",{id:e.target.value}, function(data, status){
                    console.log(1234);
                    $(".wishlist_table").html(data);
                });
            })
        })
</script>